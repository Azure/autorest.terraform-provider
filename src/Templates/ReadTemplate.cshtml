@using AutoRest.Core
@using AutoRest.Terraform
@inherits Template<ReadGenerator>

func @(Model.FunctionName)(d *schema.ResourceData, meta interface{}) error {
	client := meta.(*ArmClient).@Model.GoSDKClientName
	ctx := meta.(*ArmClient).StopContext

	id, err := parseAzureResourceID(d.Id())
	if err != nil {
		return err
	}

	resGroup := id.ResourceGroup
	name := id.Path["@Model.AzureResourceIdPath"]
	resp, err := client.Get(ctx, resGroup, name)
	if err != nil {
		if utils.ResponseWasNotFound(resp.Response) {
			d.SetId("")
			return nil
		}
		return fmt.Errorf("Error making Read request on @Model.ResourceName %q (resource group %q): %+v", name, resGroup, err)
	}

	d.Set("@Model.ResourceNameParameter", resp.Name)
	d.Set("@Model.ResourceGroupNameParameter", id.ResourceGroup)

	return nil
}
@EmptyLine